{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx","**/*.mdx", ".next/types/**/*.ts","src/middleware.ts", "src/component/CheckIcon.jsx", "src/component/Prevue.tsx", "src/component/mdx/PrevueMdx.tsx", "src/app/posts/[slug]/page.tsx", "src/component/trending/home/TrendingStory.Hometsx"],
  "exclude": ["node_modules"]
}
